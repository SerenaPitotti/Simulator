<!--case 'ModifyLink':-->
<div class='modal-dialog' role='document'>
    <div class='modal-content'>
        <div class='modal-header'>
            <h5 class='modal-title text-center'>Edit a Link<button type='button' class='close closeDrawLink' data-dismiss='modal' aria-label='Close'><span aria-hidden='true'>&times;</span></button></h5>
        </div>
        <div class='modal-body'>
            <form id='linkForm' >
                <div class='form-group'>
                    <label for='startArea' class='col-sm-5 col-form-label'>Start Area:</label>
                    <div class='col-sm-7'><p class='form-control-static' id='NomeAreaStart'>{{ NomeAreaStart }}</p></div>
                </div>
                <div class='form-group'>
                    <label for='endArea' class='col-sm-5 col-form-label'>End Area:</label>
                    <div class='col-sm-7'><p class='form-control-static' id='NomeAreaEnd'>{{ NomeAreaEnd }}</p></div>
                </div>
                <div class='form-group'>
                    <label class='col-sm-5 col-form-label' for='probProp'>Propagation Probability Link:  </label>
                    <input type='text' class='form-control' name='probProp' id='probProp' value='{{ ProbabilitaPropagazione}}' />
                </div>
                <div class='modal-footer'>
                    <button type='submit' class='btn btn-primary' >OK</button><button type='button' class='btn btn-secondary' data-dismiss='modal'>Cancel</button>
                </div>
            </form>
        </div>
    </div>
</div>
           
            
